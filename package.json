{
  "name": "js-tools",
  "version": "1.0.0",
  "description": "Js Tools",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp",
    "build-run": "gulp && node dist/index.js",
    "start": "node dist/index.js",
    "test": "gulp clean-coverage && nyc mocha -r ts-node/register -r source-map-support/register src/**/*.spec.ts"
  },
  "keywords": [
    "Node"
  ],
  "author": "Josh Walters",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/chai": "^4.1.5",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^7.0.3",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "sinon": "^7.2.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "dist/",
      "node_modules/",
      "coverage/",
      "**/*.js",
      "**/*.spec.ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  }
}
