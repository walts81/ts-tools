{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
  },
  "rules": {
    "quotemark": true,
    "arrow-parens": false,
    "interface-name": false,
    "member-access": [true, "no-public"],
    "no-namespace": false,
    "only-arrow-functions": false,
    "ordered-imports": false,
    "array-type": false,
    "no-console": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}